{
    "name": "tagadvance/pimon",
    "description": "A lightweight monitoring solution for Rasberry Pis running Ubuntu.",
    "type": "project",
    "keywords": [
        "raspberry",
        "pi",
        "monitoring"
    ],
    "homepage": "https://github.com/tagadvance/pimon",
    "license": "MIT",
    "authors": [
        {
            "name": "Tag",
            "email": "tagadvance+github.com@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/tagadvance/pimon/issues"
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4",
        "ext-bcmath": "*",
        "ext-json": "*",
        "ext-PDO": "*",
        "ext-sqlite3": "*",
        "dragonmantank/cron-expression": "^3.0",
        "guzzlehttp/guzzle": "^7.2",
        "tagadvance/elephant-top-hat": "dev-main"
    },
    "autoload" : {
        "psr-4" : {
            "" : "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    }
}
